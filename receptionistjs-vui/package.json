{
  "name": "jovo-sample-voice-app-typescript",
  "version": "2.0.2",
  "description": "A sample voice app that works with the Jovo Framework and Typescript",
  "main": "src/index.js",
  "dependencies": {
    "jovo-db-filedb": "^2.0.2",
    "jovo-framework": "^2.0.2",
    "jovo-platform-alexa": "^2.0.2",
    "jovo-platform-googleassistant": "^2.0.2",
    "jovo-plugin-debugger": "^2.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.1",
    "@types/socket.io-client": "^1.4.32",
    "gulp": "^4.0.0",
    "gulp-install": "^1.1.0",
    "gulp-run-command": "0.0.9",
    "gulp-zip": "^4.2.0",
    "jest": "^23.4.2",
    "source-map-support": "^0.5.8",
    "ts-jest": "^23.10.5",
    "tslint": "^5.10.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "scripts": {
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "tsc": "tsc",
    "typedoc": "typedoc --out ./docs ./ --mode file",
    "test": "jest",
    "bundle": "gulp --gulpfile node_modules/jovo-framework/gulpfile.js build-ts --cwd ./",
    "start": "cd dist/src && node index.js --webhook",
    "launch": "npm start -- --launch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovotech/jovo-sample-voice-app-nodejs.git"
  },
  "author": "Jovo",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jovotech/jovo-sample-voice-app-typescript/issues"
  },
  "homepage": "https://github.com/jovotech/jovo-sample-voice-app-typescript"
}
